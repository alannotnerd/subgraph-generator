{{#each contracts}}
# {{name}}
  {{#each entities}}
    {{#if isEvent}}
type {{name}}Entity @entity(immutable: true) {
  block: BigInt!
  transactionHash: Bytes!
  msgSender: Bytes! #address
  msgValue: BigInt!
    {{else}}
type {{name}} @entity(immutable: true) {
    {{/if}}
  id: ID!
    {{#each fields}}
  {{safeId name}}:{{type}}
    {{/each}}
}
  {{/each}}
{{/each}}
